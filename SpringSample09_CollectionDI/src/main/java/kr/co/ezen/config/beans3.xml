<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
		
		
		<!-- getBean()이라는 것을 호출할 때, 객체가 생성됩니다.
		고정 불변의 데이터를 처리하는 경우에 사용합니다. -->
	   <bean id="tb3" class="kr.co.ezen.beans.TestBean3" lazy-init="true">
	   		<property name="map1">
	   			<map>
	   				<entry key = "a1" value= "반갑습니다.1" />
	   				<entry key = "a2" value= "반갑습니다.2" />
	   				<entry key = "a3" value= "1000" value-type="int" />
	   				<entry key = "a4" value= "2000" value-type="int" />
	   				<entry key = "a5" value= "1000.123" value-type="double" />
	   				<entry key = "a6" value= "2000.123" value-type="double" />
	   			</map>
	   		</property>
	   		
	   </bean>
</beans>